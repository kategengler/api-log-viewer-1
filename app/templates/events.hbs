{{input placeholder="Filter by email" value=emailFilter}}
{{#link-to "events" (query-params email=emailFilter)}}Filter{{/link-to}}
<a href="#" {{action "reset"}}>Reset</a>
<div class="row">
  <div class="column">
    <ul class="events">
      {{#each event in model}}
        <li>{{#link-to "events.show" event}}{{event.occurredAt}} - {{event.description}}{{/link-to}}</li>
      {{/each}}
    </ul>
  </div>
  {{outlet}}
</div>
